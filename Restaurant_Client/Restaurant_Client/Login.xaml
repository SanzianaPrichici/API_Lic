<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Restaurant_Client.Login">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Username" />
            <Entry Placeholder="Parola" />
            <Button Text="Login!" Clicked="OnLoginClicked" />
            <Button Text="Intregistrare" Clicked="OnInregClicked" BackgroundColor="Transparent"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>