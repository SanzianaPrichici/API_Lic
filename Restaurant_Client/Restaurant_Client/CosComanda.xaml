<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Restaurant_Client.CosComanda" NavigationPage.HasNavigationBar="False" BackgroundColor="#839192">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="timp" FontSize="24" HorizontalTextAlignment="Center" TextColor="#283747"/>
            <ListView x:Name="listView" Margin="20">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell x:Name="cell" Text="{Binding Denumire}" TextColor="#283747" DetailColor="DarkOrange"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView><TableView><TableRoot><TableSection></TableSection></TableRoot></TableView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>